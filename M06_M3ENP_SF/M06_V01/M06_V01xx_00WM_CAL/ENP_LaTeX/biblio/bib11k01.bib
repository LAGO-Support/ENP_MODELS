@TECHREPORT{brock2014,
  author = {J.T. Brock and D. Hire and B. Fitzgerald and R.B. Susfalk and R.
	John},
  title = {Biscayne Aquifer Hydrology in the Vicinity of West Miami-Dade County
	Florida Water Control Structures during 2012-2013, Data Summary Report.},
  institution = {Desert Research Institute and Cardno ENTRIX},
  year = {2014},
  type = {USACE Contract No. W912HZ-12-C-0048},
  abstract = {Desert Research Institute (DRI) was contracted by the United States
	Army Corps of Engineers (USACE) to utilize an existing network of
	monitoring stations to collect data to aid in better understanding
	of the hydrology in the Biscayne Aquifer in the vicinity of the L-30
	and L-31 canals in western Miami Dade County. These data are intended
	to evaluate the hydrologic effects and effectiveness of water control
	structures on groundwater flow and surface water systems. This project
	was funded by the USACE and includes data from other partner agencies
	including the South Florida Water Management District, the Everglades
	National Park (ENP), and the United States Geological Society (USGS).
	The Biscayne Aquifer Monitoring (BAM) study area includes stations
	in the northeast corner of the ENP, the southeast corner of Water
	Conservation Area 3B (WCA 3B), and is bounded by the L30 and L31N
	canals to the east. Maps showing the locations of the facilities
	mentioned in this report are included in Figures 1-3.
	
	Rapid Creek Reach Research (RCR) and Cardno ENTRIX were subcontracted
	by DRI to support upgrades, calibration, operations, and maintenance
	of the various components of the monitoring network and to contribute
	to the acquisition, interpretation, and analysis of the monitored
	data. DRI and Cardno ENTRIX have prepared this report to provide
	a summary of the instrumentation and data collection results completed
	at the BAM Project site. The period covered by this report is from
	October 2012 through January 2014.},
  file = {:..\\references\\hydrology\\Draft_BAM_Data_2012-2013_Report_Accpt_dh30Jun2014.doc:Word},
  owner = {rfennema},
  timestamp = {2014.09.11}
}

