@ARTICLE{Genuchten1980,
  author = {M. Th. van Genuchten},
  title = {A Closed-form Equation for Predicting the Hydraulic Conductivity
	of Unsaturated Soils},
  journal = {Soil Sci. Soc. Am. J.},
  year = {1980},
  volume = {44},
  pages = {892-898},
  abstract = {A new and relatively simple equation for the soil-water content- pressure
	head curve, 8(h), is described in this paper. The particular form
	of the equation enables one to derive closedform analytical expressions
	for the relative hydraulic conductivity, Kr, when substituted in
	the predictive conductivity models of N.T. Burdine or Y. Mualem.
	The resulting expressions for Kr(h) contain three independent parameters
	which may be obtained by fitting the proposed soil-water retention
	model to experimental data. Results obtained with the closed-form
	analytical expressions based on the Mualem theory are compared with
	observed hydraulic conductivity data for five soils with a wide range
	of hydraulic properties. The unsaturated hydraulic conductivity is
	predicted well in four out of five cases. It is found that a reasonable
	description of the soil-water retention curve at low water contents
	is important for an accurate prediction of the unsaturated hydraulic
	conductivity.},
  file = {:..\\references\\peat\\vg.pdf:PDF},
  owner = {rjf},
  timestamp = {2012.02.25}
}

@TECHREPORT{geosyntec2013,
  author = {{Geosyntec Consultants}},
  title = {Geologic and Hydrologic Investigation report, West Miami-Dade Reservoir},
  institution = {Douglas Management \& Realty, Inc.},
  year = {2013},
  type = {Report},
  number = {FQ2278},
  abstract = {However, a major limitation of the model was the assumption of low
	hydraulic conductivity
	
	of the third geologic layer in which the bottom of the reservoir is
	assumed to be embedded.
	
	The model assumed a homogenous transmissivity of the third layer of
	212 feet per day
	
	(ft/day) (7.5 x 10-2 centimeters per second [cm/sec]) which is based
	on geologic borings
	
	conducted by the USGS in the 1960’s. The geology of
	
	the surficial aquifer system near the quarry site is characterized
	by peat and muck with poor
	
	natural drainage (Fish and Stewart, 1991) underlain by (from top to
	bottom) the oolitic and
	
	bryozoan limestone facies of Miami Oolite. The Miami Oolite is underlain
	by a series of
	
	highly weathered alternating shallow marine, brackish water and freshwater
	limestone facies
	
	of the Fort Thompson Formation (Causaras, 1987). The Fort Thompson
	formation in vicinity
	
	of the site is underlain by clastic and limestone units of Tamiami
	Formation and lower
	
	Hawthorn Formation which delineate the base of the surficial aquifer
	system in Dade County. It is the clastic units of the Tamiami Formation
	occurring from
	
	approximately 50 to 80 ft bls in the vicinity of the site which are
	the focus of this
	
	investigation.
	
	The grey clastic units of the Tamiami Formation generally consist
	of shelly, silty, calcareous
	
	sands (Causaras, 1987). The gray limestone, of the Tamiami Formation,
	is identified at
	
	depths of about 70 to 160 ft. bls and is lightly to moderately cemented.
	Fish and Stewart
	
	(1991) state that the gray limestone aquifer is defined as the part
	of the limestone beds and
	
	contiguous, very coarse, elastic beds that are highly permeable (e.g.,
	having hydraulic
	
	conductivity of about 100 ft/day [3.5 x 10-2 cm/sec] or greater).
	
	
	• Tamiami Formation;
	
	o Lower clastic unit of the Tamiami Formation;
	
	o Gray limestone of the Tamiami Formation (Gray Limestone Aquifer);
	
	o Upper clastic unit of the Tamiami Formation (semi confining unit);
	
	• Fort Thompson Formation (Biscayne Aquifer); and
	
	• Miami Oolite (Biscayne Aquifer).
	
	Several pumping tests were conducted by the USGS at well number G-3304
	which is located
	
	approximately two miles to the north of the proposed reservoir site.
	These tests indicated
	
	hydraulic conductivities of 40,000 (14 cm/sec) and 240 ft/day (8.4
	x 10-2 cm/sec) at intervals
	
	of 30-55 ft bls (Biscayne Aquifer) and 80-95.5 ft bls (Gray Limestone
	Aquifer), respectively.
	
	Review of boring logs presented in Wolf/WPC (2008), advanced approximately
	three miles to
	
	the north of the proposed reservoir, shows that the subsurface is
	generally characterized by
	
	weathered limestone at depths of approximately 30 ft bls to highly
	weathered limestone at
	
	depths of approximately 35 ft bls to 75 ft bls underlain by clayey
	loose sands (average 75 to
	
	80 ft bls) to poorly graded dense to medium dense sands with traces
	of clay and shells. Note
	
	that the characterization presented by Fish and Stewart (1991) is
	in general agreement with
	
	those presented in Wolf/WPC (2008) with the exception that the lower
	permeability material
	
	is encountered at 75 ft bls rather than the approximately 95 and 110
	ft bls as referenced by
	
	Fish and Stewart (1991). During this investigation, a pumping test
	was conducted on the
	
	interval between 17 and 72 ft bls in the Biscayne Aquifer which yielded
	a hydraulic
	
	conductivity of approximately 50,000 ft/day (18 cm/sec).},
  file = {:..\\references\\hydrology\\Q13050_WMDR Report_DRAFT_1.pdf:PDF},
  owner = {rfennema},
  timestamp = {2014.09.12}
}

